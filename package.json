{
  "name": "breakdancer",
  "version": "4.0.0",
  "description": "A breakpoint tracking utility",
  "main": "./lib",
  "browser": "./lib",
  "module": "./index",
  "scripts": {
    "lint": "eslint-godaddy *.js",
    "test:mocha": "mocha --require @babel/register ./test.js",
    "build": "babel *.js -d ./lib",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "nyc --reporter=text --reporter=json-summary npm run test:mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/breakdancer.git"
  },
  "keywords": [
    "breakpoints",
    "breakdancer",
    "breakpoint",
    "tracking",
    "break",
    "point"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "assume": "^2.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.16.0",
    "eslint-config-godaddy": "^4.0.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-native": "^0.58.6",
    "react-native-mock": "^0.3.1"
  },
  "pre-commit": "lint, test",
  "dependencies": {
    "propget": "^1.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  }
}
